<svg width="1200" height="800" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="diffGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2E5BA8;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1A3D7C;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <rect width="1200" height="800" fill="url(#diffGrad)"/>
  
  <!-- Excellence/quality symbols -->
  <g transform="translate(300, 300)" opacity="0.5">
    <!-- Trophy/award shape -->
    <path d="M 50,100 L 100,50 L 150,100 L 140,180 L 60,180 Z" fill="none" stroke="#F26522" stroke-width="5"/>
    <rect x="75" y="180" width="50" height="40" fill="none" stroke="#F26522" stroke-width="4"/>
    <line x1="60" y1="220" x2="140" y2="220" stroke="#F26522" stroke-width="8" stroke-linecap="round"/>
    
    <!-- Star for excellence -->
    <path d="M 100,20 L 110,45 L 137,47 L 118,67 L 123,94 L 100,80 L 77,94 L 82,67 L 63,47 L 90,45 Z" fill="#F26522"/>
  </g>
  
  <!-- Growth/upward trend arrow -->
  <g transform="translate(700, 400)" opacity="0.5">
    <polyline points="0,150 80,120 160,80 240,20" fill="none" stroke="#F26522" stroke-width="6" stroke-linejoin="round"/>
    <polygon points="240,20 220,35 230,50" fill="#F26522"/>
    
    <!-- Data points -->
    <circle cx="0" cy="150" r="10" fill="white"/>
    <circle cx="80" cy="120" r="10" fill="white"/>
    <circle cx="160" cy="80" r="10" fill="white"/>
    <circle cx="240" cy="20" r="12" fill="#F26522"/>
  </g>
  
  <!-- Partnership handshake symbol -->
  <g transform="translate(200, 600)" opacity="0.4">
    <path d="M 20,50 L 50,20 L 80,50 L 120,50 L 150,20 L 180,50" fill="none" stroke="white" stroke-width="8" stroke-linejoin="round"/>
  </g>
  
  <text x="600" y="750" font-size="44" fill="white" text-anchor="middle" font-family="Inter, Arial, sans-serif" font-weight="600">What Sets Us Apart</text>
</svg>
